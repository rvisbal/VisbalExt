{
  "name": "visbal-ext",
  "displayName": "Visbal Extension",
  "version": "0.0.1",
  "description": "A basic extension for Cursor with find functionality",
  "main": "dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/visbal-ext.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "package": "vsce package"
  },
  "engines": {
    "vscode": "^1.63.0",
    "cursor": "^0.1.0"
  },
  "activationEvents": [
    "onCommand:visbal-ext.helloWorld",
    "onCommand:visbal-ext.showFindModel",
    "onCommand:visbal-ext.showLogSummary",
    "onView:visbalLogView",
    "onView:testClassExplorerView",
    "onLanguage:log"
  ],
  "contributes": {
    "commands": [
      {
        "command": "visbal-ext.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "visbal-ext.showFindModel",
        "title": "Show Find Model"
      },
      {
        "command": "visbal-ext.showLogSummary",
        "title": "Show Log Summary",
        "icon": "$(notebook)"
      },
      {
        "command": "visbal-ext.refreshVisbalLog",
        "title": "Refresh Visbal Log",
        "icon": "$(refresh)"
      },
      {
        "command": "visbal.fetchLogsViaRestApi",
        "title": "Fetch Salesforce Logs via REST API",
        "icon": "$(cloud-download)"
      },
      {
        "command": "visbal.executeApexRest",
        "title": "Execute Salesforce Apex REST Endpoint",
        "icon": "$(play)"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "visbalLogContainer",
          "title": "Visbal Log",
          "icon": "$(notebook)"
        }
      ],
      "activitybar": [
        {
          "id": "testExplorerContainer",
          "title": "Salesforce Test Explorer",
          "icon": "$(beaker)"
        }
      ]
    },
    "views": {
      "visbalLogContainer": [
        {
          "id": "visbalLogView",
          "name": "Log Analyzer",
          "type": "webview"
        }
      ],
      "testExplorerContainer": [
        {
          "id": "testClassExplorerView",
          "name": "Test Classes",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "visbal-ext.showFindModel",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "visbal-ext.showLogSummary",
          "group": "navigation",
          "when": "resourceExtname == .log"
        }
      ],
      "view/title": [
        {
          "command": "visbal-ext.refreshVisbalLog",
          "when": "view == visbalLogView",
          "group": "navigation"
        },
        {
          "command": "visbal.fetchLogsViaRestApi",
          "when": "view == visbalLogView",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "visbal.fetchLogsViaRestApi",
          "group": "Salesforce"
        },
        {
          "command": "visbal.executeApexRest",
          "group": "Salesforce"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.63.0",
    "typescript": "^4.5.5",
    "vsce": "^2.7.0"
  },
  "dependencies": {
    "axios": "^1.8.3"
  }
}
