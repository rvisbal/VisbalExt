{
  "name": "visbal-ext",
  "displayName": "Visbal Extension",
  "version": "0.1.0",
  "publisher": "visbal",
  "description": "A Visual Studio Code extension for Salesforce development with enhanced test execution and log analysis capabilities",
  "author": {
    "name": "Visbal"
  },
  "categories": [
    "Other",
    "Testing",
    "Debuggers"
  ],
  "keywords": [
    "salesforce",
    "apex",
    "testing",
    "logs",
    "debug"
  ],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/visbal-ext.git"
  },
  "scripts": {
    "clean": "rimraf dist out *.vsix",
    "build": "tsc",
    "watch": "tsc -w",
    "package": "vsce package",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "prepackage": "npm run clean && npm run build"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "onView:testClassExplorerView",
    "onView:visbal-log",
    "onView:visbal-soql",
    "onView:visbal-apex",
    "workspaceContains:**/.sfdx/sfdx-config.json",
    "workspaceContains:**/sfdx-project.json"
  ],
  "contributes": {
    "viewsContainers": {
      "debug": [],
      "panel": [
        {
          "id": "visbal-log-container",
          "title": "Visbal Log",
          "icon": "$(notebook)"
        },
        {
          "id": "visbal-soql-container",
          "title": "Visbal SOQL",
          "icon": "$(database)"
        },
        {
          "id": "visbal-apex-container",
          "title": "Visbal Apex",
          "icon": "$(symbol-method)"
        }
      ],
      "activitybar": [
        {
          "id": "testExplorerContainer",
          "title": "Visbal Test",
          "icon": "$(beaker)"
        }
      ]
    },
    "views": {
      "debug": [
        {
          "type": "webview",
          "id": "visbal-test-results",
          "name": "Test Results",
          "when": "inDebugMode"
        }
      ],
      "testExplorerContainer": [
        {
          "id": "testClassExplorerView",
          "name": "Test Classes",
          "type": "webview"
        }
      ],
      "visbal-log-container": [
        {
          "type": "webview",
          "id": "visbal-log",
          "name": "Log Analyzer"
        },
        {
          "id": "logTreeView",
          "name": "Log Tree",
          "type": "tree"
        }
      ],
      "visbal-soql-container": [
        {
          "type": "webview",
          "id": "visbal-soql",
          "name": "SOQL Query"
        }
      ],
      "visbal-apex-container": [
        {
          "type": "webview",
          "id": "visbal-apex",
          "name": "Execute Anonymous Apex"
        }
      ]
    },
    "commands": [
      {
        "command": "visbal.clearConsole",
        "title": "Clear Console",
        "category": "Visbal",
        "icon": "$(clear-all)"
      },
      {
        "command": "visbal-ext.refreshVisbalLog",
        "title": "Refresh Visbal Log",
        "category": "Visbal",
        "icon": "$(refresh)"
      },
      {
        "command": "visbal.fetchLogsViaRestApi",
        "title": "Fetch Salesforce Logs via REST API",
        "icon": "$(cloud-download)"
      },
      {
        "command": "visbal.executeApexRest",
        "title": "Execute Salesforce Apex REST Endpoint",
        "icon": "$(play)"
      },
      {
        "command": "visbal-ext.showVisbalLog",
        "title": "Show Visbal Log",
        "category": "Visbal"
      },
      {
        "command": "visbal-ext.showVisbalSoql",
        "title": "Show Visbal SOQL",
        "category": "Visbal"
      },
      {
        "command": "visbal-ext.showVisbalApex",
        "title": "Show Visbal Apex",
        "category": "Visbal"
      },
      {
        "command": "visbal-ext.showDebugConsole",
        "title": "Show Debug Console",
        "category": "Visbal Debug"
      },
      {
        "command": "visbal-ext.showTestResults",
        "title": "Show Test Results",
        "category": "Visbal Debug"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "visbal.clearConsole",
          "group": "navigation"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "@vscode/vsce": "^2.24.0"
  },
  "dependencies": {
    "@salesforce/core": "^5.3.9",
    "@vscode/codicons": "^0.0.35",
    "axios": "^1.6.7",
    "jsforce": "^1.11.1"
  }
}
